{% extends "base.html.twig" %}
{% block body %}

<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">Test</th>
      <th scope="col">Time</th>
      <th scope="col">CPU</th>
      <th scope="col">Size</th>
    </tr>
  </thead>
  <tbody>
    {% for key, ArrayTestResult in ArrayTestResults %}
        <tr>
        <th scope="row">{{ ArrayTestResult.TestName }}</th>
        <td>{{ ArrayTestResult.Time }}</td>
        <td>{{ ArrayTestResult.CPU }}</td>
        <td>Not implemented yet</td>
        </tr>
    {% endfor %}
</table>
<div class="container">
    <a href="{{ path('start') }}" class="btn btn-secondary btn-lg btn-block">Get Back</a>
</div>
{% endblock %}